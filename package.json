    "expo-updates": "~0.11.6",
    "firebase": "^9.6.10",
    "jest-expo": "^45.0.0",
    "react": "17.0.1",
    "react-dom": "^17.0.1",
    "react-native": "0.64.3",
    "react-native-deck-swiper": "^2.0.5",
